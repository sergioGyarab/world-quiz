{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "dev",
      "path": "FrontEnd/",
      "label": "npm: dev - FrontEnd",
      "isBackground": true,
      "problemMatcher": {
        "owner": "typescript",
        "pattern": {
          "regexp": "^([^\\s].*)\\((\\d+|\\d+,\\d+|\\d+,\\d+,\\d+,\\d+)\\):\\s+(error|warning|info)\\s+(TS\\d+)\\s*:\\s*(.*)$",
          "file": 1,
          "location": 2,
          "severity": 3,
          "code": 4,
          "message": 5
        },
        "background": {
          "activeOnStart": true,
          "beginsPattern": ".*",
          "endsPattern": "(Local|ready in)"
        }
      }
    },
    {
      "type": "npm",
      "script": "build",
      "path": "FrontEnd/",
      "label": "npm: build - FrontEnd",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$tsc"]
    },
    {
      "type": "npm",
      "script": "preview",
      "path": "FrontEnd/",
      "label": "npm: preview - FrontEnd",
      "isBackground": true
    }
  ]
}
